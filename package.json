{
  "name": "hackathon",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "cross-env NODE_ENV=test npx jest --config jest.config.js --runInBand --detectOpenHandles",
    "test:w": "cross-env NODE_ENV=test npx jest --config jest.config.js --runInBand --detectOpenHandles --watch",
    "dev": "nodemon ./src/server.js",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:undo": "npx sequelize-cli db:migrate:undo:all"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.18.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.7",
    "@sucrase/jest-plugin": "^2.2.0",
    "cross-env": "7.0.3",
    "jest": "^27.4.5",
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.4.1",
    "sqlite3": "^5.0.2",
    "sucrase": "^3.20.3",
    "supertest": "6.1.6"
  }
